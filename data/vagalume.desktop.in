[Desktop Entry]
Version=1.0
Type=Application
Categories=Audio;AudioVideo;GNOME;GTK;Application;Player;
Name=Vagalume Last.fm Client
Exec=@BINDIR@/vagalume
Icon=vagalume
X-Icon-path=@ICONDIR@
X-Window-Icon=vagalume
X-HildonDesk-ShowInToolbar=true
X-Osso-Service=com.igalia.vagalume
X-Osso-Type=application/x-executable
X-Osso-URI-Actions=lastfm;

[X-Osso-URI-Action Handler lastfm]
Method=PlayUrl
Name=uri_link_play
TranslationDomain=osso-uri
